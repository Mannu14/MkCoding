  </section>
  <section class="contact-us">
    <div class="row">
        <div class="contact-col">
            <div>
                <i class="fa fa-home"></i>
                <span>
                    <h5>Khatu Road, ABC Building</h5>
                    <p>sikar, Rajasthan, IN</p>
                </span>
            </div>
            <div>
                <i class="fa fa-phone"></i>
                <span>
                    <h5>+1 0123456789</h5>
                    <p>Monday to Saturday,10AM to 6PM</p>
                </span>
            </div>
            <div>
                <i class="fa fa-envelope-o"></i>
                <span>
                    <h5>enfo@gmail.com</h5>
                    <p>Email Us youe query</p>
                </span>
            </div>
        </div>   
    </div>
    <div class="container">
        <h1>Contact Us</h1>
        <form onsubmit="formreload()" id="blankform" target="_blank"  action="/sendemail" method="post">
            <div class="form-group">
                <label for="name">Name:</label>
                <input type="text" id="name" name="name" autocomplete="off" required>
            </div>
            <div class="form-group">
                <label for="email">Email:</label>
                <input type="email" id="email" name="email" autocomplete="off" required>
            </div>
            <div class="form-group">
                <label for="email">Subject:</label>
                <input type="text" id="subject" name="subject" autocomplete="off" required>
            </div>
            <div class="form-group">
                <label for="number">Phone No. :</label>
                <input type="number" id="number" name="number" autocomplete="off" required>
            </div>
            <div class="form-group">
                <label for="message">Message:</label>
                <textarea id="message" name="message" rows="5" autocomplete="off" required></textarea>
            </div>
            <button id="send-btn" type="submit">Send</button>
        </form>
    </div>
  </section>
  <button onclick="topFunction()" id="myBtn" title="Go to top">Top</button>
    <link rel="stylesheet" href="css/top.css">
    <script src="js/top.js"></script>
   
  <section class="footer">
