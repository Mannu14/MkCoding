<%
let i = 1;
for (let item = 0; item < items.length; item++) {
%>
<div class="maindiv" data-name="<%= items[item].uploadClass %>">
<h2 name="mainheading_h2" class="mainheading_h2"><a href=""><%= items[item].mainheading_h2%></a></h2>
<div id="headingImage" name="mainheading_file_2" class="mainheading_file_2 hidden-img blur-load"><img loading="lazy" src="images/black-backGround.png" data-src="data:image/<%=items[item].img.contentType%>;base64,
<%=items[item].img.data.toString('base64')%>" name="image" /></div>
<div class="code-label-divs">
<h3 name="mainheading_h3" class="mainheading_h3"><%= items[item].mainheading_h3%></h3>
<div class="code code-label-div"><br>
<label class="copy-run-label"> <a class="online-run" href="https://mkcoding.online/OnlineCompiler">Run</a> </label>
<label class="copy-run-label"><button class="copy-button" data-target='editor-<%= i %>'>Copy</button></label>
<textarea class="editor"name="textarea" id='editor-<%= i %>' cols="600" rows="100"><%= items[item].textarea_h4 %></textarea>
</div>
<h6 name="mainheading_h6" class="mainheading_h6"><%= items[item].mainheading_h6%></h6>
<div class="code code-label-div"><br>
<label class="copy-run-label"><a class="online-run" href="https://mkcoding.online/OnlineCompiler">Run</a></label>
<label class="copy-run-label"><button class="copy-button" data-target='editor-<%= i + 1 %>'>Copy</button></label>
<textarea class="editor"name="textarea" id='editor-<%= i + 1 %>' cols="600" rows="100"><%= items[item].textarea_h5 %></textarea>
</div>
<p name="mainheading_p" class="mainheading_p"><%= items[item].mainheading_p%></p>
<div class="code code-label-div"><br>
<label class="copy-run-label"><a class="online-run" href="https://mkcoding.online/OnlineCompiler">Run</a></label>
<label class="copy-run-label"><button class="copy-button" data-target='editor-<%= i + 2 %>'>Copy</button></label>
<textarea class="editor"name="textarea" id='editor-<%= i + 2 %>' cols="600" rows="100"><%= items[item].textarea %></textarea>
</div>
</div>
</div>
<%  
i += 3;
} %>
</div>
