<script>
document.addEventListener('DOMContentLoaded', function () {
const projectLinks = document.querySelectorAll('.code-label-divs .online-run');
projectLinks.forEach(projectLink => {
  projectLink.addEventListener('click', function (event) {
  event.preventDefault();
  const currentURL = 'https://mkcoding.online/OnlineCompiler';
  const h3Element = projectLink.closest('.maindiv').querySelector('h3');
  const h3Value = h3Element.textContent;
  if (currentURL == 'https://mkcoding.online/OnlineCompiler') {
    updatePage(h3Value, currentURL);
};});});
function updatePage(page, currentURL) {
if (currentURL) {
  const url = new URL(currentURL, window.location.origin);
  url.searchParams.set('Manish_search', page);
  window.location.href = url.toString();
};};});
</script>