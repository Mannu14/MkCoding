    <script>
        function showtext() {
            var i = 1;
            while (document.getElementById(`editor-${i}`)) {
                var editor = Mkcoding.fromTextArea(document.getElementById(`editor-${i}`), {
                    mode: "text/x-python",
                    theme: "dracula",
                    lineNumbers: true,
